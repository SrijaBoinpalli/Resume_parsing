    "import tika\n",
    "from tika import parser\n",
    "import re\n",
    "class ResumeParsing:\n",
    "    def parseAnyFile():\n",
    "        tika.initVM()\n",
    "        parsed = parser.from_file(\"Srija Boinpalli Resume.docx\")\n",
    "        print(parsed[\"content\"])\n",
    "        regular_expression = re.compile(r\"[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,4}\", re.IGNORECASE)\n",
    "        mailid = re.findall(regular_expression,parsed[\"content\"])\n",
    "        print("E-mail=",mailid)\n",
    "        regular_expression = re.compile(r\"\\+\\d{2}\\s?0?\\d{10}\", re.IGNORECASE)\n",
    "        phone = re.findall(regular_expression,parsed[\"content\"])\n",
    "        print("Phone number=",phone)\n",
    "        regular_expression = re.compile(r\"[0-9-]* (.*),(.*) [A-Z]{2,3} [0-9]{6}\", re.IGNORECASE)\n",
    "        address = re.search(regular_expression,parsed[\"content\"]) \n",
    "        print("Address=",address) #Extraction of address depends on the format of address given in your document\n",
    "    if __name__==\"__main__\":\n",
    "        parseAnyFile()"
   ]
